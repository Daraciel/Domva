<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewRes.SharedStrings.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-2.5.3.js")" type="text/javascript"></script>
</head>
<body>
    @{
        if (Session["User"] == null)
        {
            Response.Redirect("~/Usuarios/Login");
        }
        }
    <div class="page">
        <header>
            <div id="title">
                <h1>DomvAlicante</h1>
            </div>
            <nav>
                <ul id="menu">
                    <li>@Html.ActionLink("Inicio", "Index", "Home")</li>
                    <li>@Html.ActionLink("Propiedades", "Index", "Propiedades")</li>
                    <li>@Html.ActionLink("Propietarios", "Index", "Propietarios")</li>
                    <li>@Html.ActionLink("Fotos", "Index", "Fotos")</li>
                    <li>@Html.ActionLink("Acerca de", "About", "Home")</li>
                    <li>@Html.ActionLink("Pagina Pública", "Index", "Public")</li>
                    <!-- <li>@Html.ActionLink("Grid", "Grid", "Home")</li> -->
                </ul>
            </nav>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
        </footer>
    </div>
</body>
</html>
