@model IEnumerable<domvaproject.fotos>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            propiedades
        </th>
        <th>
            Propiedad
        </th>
        <th>
            Principal
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.propiedades.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Propiedad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Principal)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id= item.idFoto, propiedad= item.Propiedad  }) |
            @Html.ActionLink("Details", "Details", new { id= item.idFoto, propiedad= item.Propiedad }) |
            @Html.ActionLink("Delete", "Delete", new { id= item.idFoto, propiedad= item.Propiedad })
        </td>
    </tr>
}

</table>
