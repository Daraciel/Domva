@model domvaproject.descripcionesesp

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>descripcionesesp</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.Propiedad, "propiedades")
        </div>
        <div class="editor-field">
            @Html.DropDownList("Propiedad", String.Empty)
            @Html.ValidationMessageFor(model => model.Propiedad)
        </div>

        <div class="editor-label">
            Descripción en Español
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.DescripcionESP)
            @Html.ValidationMessageFor(model => model.DescripcionESP)
        </div>

        <p>
            <input type="submit" value="Crear" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Atrás", "Index")
</div>
